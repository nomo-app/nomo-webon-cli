{
  "name": "nomo-webon-cli",
  "version": "0.1.1",
  "description": "A CLI for building and deploying Nomo WebOns",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nomo-app/nomo-webon-cli.git"
  },
  "homepage": "https://github.com/nomo-app/nomo-webon-cli#readme",
  "bin": {
    "nomo-webon-cli": "bin/nomo-webon-cli"
  },
  "main": "./dist/index.js",
  "files": [
    "bin/",
    "dist/"
  ],
  "scripts": {
    "build": "tsc",
    "link": "npm link",
    "clean": "rm -rf ./dist",
    "test": "jest --testTimeout=10000",
    "prepublishOnly": "npm run build && git diff --exit-code",
    "deploy": "nomo-webon-cli build test_assets && nomo-webon-cli deploy out/nomo.tar.gz production"
  },
  "dependencies": {
    "commander": "^6.1.0",
    "inquirer": "^7.3.3",
    "semver": "^7.5.4",
    "tar": "^6.2.0",
    "tslib": "^2.6.2"
  },
  "devDependencies": {
    "@types/inquirer": "^7.3.3",
    "@types/jest": "^29.5.8",
    "@types/node": "^14.11.8",
    "@types/semver": "^7.5.0",
    "@types/tar": "^6.1.9",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2"
  },
  "jest": {
    "preset": "ts-jest",
    "roots": [
      "test"
    ]
  }
}
